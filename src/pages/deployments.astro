---
import Layout from "../layouts/Layout.astro";
import VerticalNavigation from "../components/VerticalNavigation.astro";
import BMRCLChampsGallery from "../components/BMRCLChampsGallery.jsx";

// Import all partner logos
import bmrclLogo from "../assets/partners/bmrcl.svg";
import metaLogo from "../assets/partners/meta.svg";
import dbsLogo from "../assets/partners/development-bank-of-singapore.svg";
import southIndianBankLogo from "../assets/partners/south-indian-bank.png";
import nsdlLogo from "../assets/partners/nsdl.png";
import axisBankLogo from "../assets/partners/axis-bank-logo.svg";
import kotakLifeLogo from "../assets/partners/kotak-life-insurance.png";
import veenaWorldLogo from "../assets/partners/veena_world.svg";
import adityaVisionLogo from "../assets/partners/aditya_vision.png";
import saybaGroupLogo from "../assets/partners/sayba_group.png";
import aumInfotechLogo from "../assets/partners/aum_infotech.jpeg";
import advantechLogo from "../assets/partners/Advantech.svg";
import dataaegisLogo from "../assets/partners/dataaegis.png";
import airtelPaymentsBankLogo from "../assets/partners/airtel_payments_bank.svg";
import rupayLogo from "../assets/partners/rupay.png";
import tvm4000BrochureVideo from "../assets/kiosks/TVM 4000/tvm_4000_brochure.mp4";
// Import BMRCL Champs images
import bmrclChamps1 from "../assets/bmrcl_champs/IMG-20251112-WA0016.jpg";
import bmrclChamps2 from "../assets/bmrcl_champs/IMG-20251112-WA0018.jpg";
import bmrclChamps3 from "../assets/bmrcl_champs/IMG-20251112-WA0019.jpg";
import bmrclChamps4 from "../assets/bmrcl_champs/IMG-20251112-WA0020.jpg";
import bmrclChamps5 from "../assets/bmrcl_champs/IMG-20251112-WA0021.jpg";
import bmrclChamps6 from "../assets/bmrcl_champs/IMG-20251112-WA0022.jpg";
import bmrclChamps7 from "../assets/bmrcl_champs/WhatsApp Image 2025-11-12 at 19.52.06_1bd28dcc.jpg";
import bmrclChamps8 from "../assets/bmrcl_champs/WhatsApp Image 2025-11-12 at 19.53.23_f38854d0.jpg";
import bmrclChamps9 from "../assets/bmrcl_champs/WhatsApp Image 2025-11-12 at 19.53.58_188d61fb.jpg";
import bmrclChamps10 from "../assets/bmrcl_champs/WhatsApp Image 2025-11-12 at 20.22.24_a4987cb5.jpg";
---

<Layout title="Frog8 - Deployments | Real-World Success Stories">
  <VerticalNavigation />

  <!-- Hero Section -->
  <section class="section gradient-bg pt-32 pb-10 md:pb-16">
    <div class="container">
      <div class="text-center mb-12 md:mb-16">
        <h1
          class="fade-in mb-4 md:mb-6 text-3xl md:text-5xl font-bold tracking-tight"
        >
          Proven <span class="text-green">Deployments</span>
        </h1>
        <p
          class="fade-in-delay text-lg md:text-xl max-w-3xl mx-auto text-white/90"
        >
          Real-world success stories across metro networks, banks, and
          enterprises. See how Frog8 is powering India's infrastructure
          transformation.
        </p>
      </div>
    </div>
  </section>

  <div class="section-divider"></div>

  <!-- Partner Grid -->
  <section class="section partners-section bg-glass-light">
    <div class="container">
      <h2 class="text-center mb-6 md:mb-8">
        Trusted <span class="text-green">Partners</span>
      </h2>
      <div class="partners-grid-wrapper">
        {
          [
            {
              name: "Metro/BMRCL",
              desc: "Transit infrastructure",
              logo: bmrclLogo.src
            },
            {
              name: "Meta",
              desc: "Technology partnerships",
              logo: metaLogo.src
            },
            {
              name: "DBS Bank",
              desc: "Digital banking solutions",
              logo: dbsLogo.src
            },
            {
              name: "South Indian Bank",
              desc: "Banking automation",
              logo: southIndianBankLogo.src
            },
            {
              name: "Airtel Payments Bank",
              desc: "Digital payments infrastructure",
              logo: airtelPaymentsBankLogo.src
            },
            {
              name: "Dataaegis",
              desc: "Data security solutions",
              logo: dataaegisLogo.src
            },
            {
              name: "Axis Bank",
              desc: "Financial services",
              logo: axisBankLogo.src
            },
            {
              name: "Kotak Life",
              desc: "Insurance onboarding",
              logo: kotakLifeLogo.src
            },
            {
              name: "Veena World",
              desc: "Travel tech solutions",
              logo: veenaWorldLogo.src
            },
            {
              name: "Aditya Vision",
              desc: "Retail automation",
              logo: adityaVisionLogo.src
            },
            {
              name: "Sayba Group",
              desc: "Enterprise solutions",
              logo: saybaGroupLogo.src
            },
            {
              name: "AUM Infotech",
              desc: "Asset management",
              logo: aumInfotechLogo.src
            },
            {
              name: "Advantech",
              desc: "Asset management",
              logo: advantechLogo.src
            }
          ].map(({name, desc, logo}) => (
            <div class="partner-card glass text-center hover-lift rounded-2xl border border-green/10 shadow-lg bg-white/10">
              <div class="partner-logo-container">
                <img 
                  src={logo} 
                  alt={`${name} logo`}
                  class="partner-logo"
                />
              </div>
              <h5 class="text-green">
                {name}
              </h5>
              <p>{desc}</p>
            </div>
          ))
        }
      </div>
    </div>
  </section>

  <div class="section-divider"></div>

  <!-- Featured Deployments -->
  <section class="section bg-black/20">
    <div class="container">
      <h2 class="text-center mb-16">
        Featured <span class="text-green">Deployments</span>
      </h2>
      <div class="featured-deployments-grid">
        <!-- Card 1 -->
        <div class="featured-deployment-card glass">
          <div class="deployment-icon-container">
            <div class="deployment-icon-wrapper">
              <svg width="56" height="56" fill="none" viewBox="0 0 96 96"
                ><circle cx="40" cy="70" r="10" fill="#a7d221"></circle><rect
                  x="16"
                  y="24"
                  width="44"
                  height="36"
                  rx="10"
                  fill="#a7d221"></rect><rect
                  x="64"
                  y="40"
                  width="20"
                  height="20"
                  rx="7"
                  fill="#a7d221"></rect><circle
                  cx="74"
                  cy="70"
                  r="10"
                  fill="#a7d221"></circle></svg
              >
            </div>
          </div>
          <h3 class="deployment-title">
            BMRCL WhatsApp<br />Ticketing
          </h3>
          <p class="deployment-description">
            Revolutionary WhatsApp-based metro ticketing system serving 100K+
            daily transactions
          </p>
          <ul class="deploy-bullets">
            <li>
              <svg class="deploy-bullet-icon" width="16" height="16" viewBox="0 0 16 16" fill="none">
                <circle cx="8" cy="8" r="6" fill="#a7d221"/>
              </svg>
              <span>100K+ daily transactions</span>
            </li>
            <li>
              <svg class="deploy-bullet-icon" width="16" height="16" viewBox="0 0 16 16" fill="none">
                <circle cx="8" cy="8" r="6" fill="#a7d221"/>
              </svg>
              <span>WhatsApp-native experience</span>
            </li>
            <li>
              <svg class="deploy-bullet-icon" width="16" height="16" viewBox="0 0 16 16" fill="none">
                <circle cx="8" cy="8" r="6" fill="#a7d221"/>
              </svg>
              <span>NCMC compliance</span>
            </li>
            <li>
              <svg class="deploy-bullet-icon" width="16" height="16" viewBox="0 0 16 16" fill="none">
                <circle cx="8" cy="8" r="6" fill="#a7d221"/>
              </svg>
              <span>Multi-modal payments</span>
            </li>
          </ul>
        </div>
        <!-- Card 2 -->
        <div class="featured-deployment-card glass">
          <div class="deployment-icon-container">
            <div class="deployment-icon-wrapper">
              <svg width="56" height="56" fill="none" viewBox="0 0 96 96"
                ><rect x="16" y="24" width="64" height="16" rx="8" fill="#a7d221"
                ></rect><rect
                  x="16"
                  y="48"
                  width="32"
                  height="32"
                  rx="8"
                  fill="#a7d221"></rect><rect
                  x="56"
                  y="56"
                  width="24"
                  height="24"
                  rx="8"
                  fill="#a7d221"></rect></svg
              >
            </div>
          </div>
          <h3 class="deployment-title">Transit TVMs</h3>
          <p class="deployment-description">
            Ticket Vending Machines deployed at key metro stations with 24/7
            reliability
          </p>
          <ul class="deploy-bullets">
            <li>
              <svg class="deploy-bullet-icon" width="16" height="16" viewBox="0 0 16 16" fill="none">
                <circle cx="8" cy="8" r="6" fill="#a7d221"/>
              </svg>
              <span>50+ stations deployed</span>
            </li>
            <li>
              <svg class="deploy-bullet-icon" width="16" height="16" viewBox="0 0 16 16" fill="none">
                <circle cx="8" cy="8" r="6" fill="#a7d221"/>
              </svg>
              <span>99.9% uptime achieved</span>
            </li>
            <li>
              <svg class="deploy-bullet-icon" width="16" height="16" viewBox="0 0 16 16" fill="none">
                <circle cx="8" cy="8" r="6" fill="#a7d221"/>
              </svg>
              <span>Weather-resistant design</span>
            </li>
            <li>
              <svg class="deploy-bullet-icon" width="16" height="16" viewBox="0 0 16 16" fill="none">
                <circle cx="8" cy="8" r="6" fill="#a7d221"/>
              </svg>
              <span>Real-time monitoring</span>
            </li>
          </ul>
        </div>
        <!-- Card 3 -->
        <div class="featured-deployment-card glass">
          <div class="deployment-icon-container">
            <div class="deployment-icon-wrapper">
              <svg width="56" height="56" fill="none" viewBox="0 0 96 96"
                ><rect x="14" y="26" width="50" height="50" rx="16" fill="#a7d221"
                ></rect><rect
                  x="32"
                  y="44"
                  width="50"
                  height="36"
                  rx="16"
                  fill="#a7d221"></rect></svg
              >
            </div>
          </div>
          <h3 class="deployment-title">CTS Banking</h3>
          <p class="deployment-description">
            Check Truncation System deployment across multiple banks for digital
            transformation
          </p>
          <ul class="deploy-bullets">
            <li>
              <svg class="deploy-bullet-icon" width="16" height="16" viewBox="0 0 16 16" fill="none">
                <circle cx="8" cy="8" r="6" fill="#a7d221"/>
              </svg>
              <span>RBI-compliant processing</span>
            </li>
            <li>
              <svg class="deploy-bullet-icon" width="16" height="16" viewBox="0 0 16 16" fill="none">
                <circle cx="8" cy="8" r="6" fill="#a7d221"/>
              </svg>
              <span>Document digitization</span>
            </li>
            <li>
              <svg class="deploy-bullet-icon" width="16" height="16" viewBox="0 0 16 16" fill="none">
                <circle cx="8" cy="8" r="6" fill="#a7d221"/>
              </svg>
              <span>Automated workflows</span>
            </li>
            <li>
              <svg class="deploy-bullet-icon" width="16" height="16" viewBox="0 0 16 16" fill="none">
                <circle cx="8" cy="8" r="6" fill="#a7d221"/>
              </svg>
              <span>Banking integration</span>
            </li>
          </ul>
        </div>
      </div>
    </div>
  </section>

  <div class="section-divider"></div>

  <!-- BMRCL Journey Section -->
  <section class="section bg-black/20">
    <div class="container">
      <div class="text-center mb-16">
        <h2 class="slide-up mb-6">
          Our Journey with <span class="text-green">BMRCL</span>
        </h2>
        <p class="text-xl max-w-3xl mx-auto">
          A three-year evolution from digital-first to full-platform deployment
        </p>
      </div>

      <!-- Timeline -->
      <div class="journey-timeline">
        <!-- Timeline Item 1 -->
        <div class="timeline-item">
          <div class="timeline-marker"></div>
          <div class="timeline-content glass">
            <div class="timeline-date">April 2022</div>
            <h3 class="timeline-title">WhatsApp Ticketing Launch</h3>
            <p class="timeline-description">
              Digital-first approach. Scaling to 100K tickets per day. Smart card top-ups and ticketing accessible via WhatsApp.
            </p>
          </div>
        </div>

        <!-- Timeline Item 2 -->
        <div class="timeline-item">
          <div class="timeline-marker"></div>
          <div class="timeline-content glass">
            <div class="timeline-date">April 2024</div>
            <h3 class="timeline-title">TVM 1000 Pilot</h3>
            <p class="timeline-description">
              First hardware deployment at two of Bengaluru's busiest stations: Cubbon Park and MG Road. Introduction of self-service QR ticket machines.
            </p>
          </div>
        </div>

        <!-- Timeline Item 3 -->
        <div class="timeline-item">
          <div class="timeline-marker"></div>
          <div class="timeline-content glass">
            <div class="timeline-date">Q1 2025</div>
            <h3 class="timeline-title">TVM 2000 Expansion</h3>
            <p class="timeline-description">
              Industrial-grade, all-digital-payment TVMs deployed at four additional stations: Byappanahalli, Benniganahalli, SV Road, and Raggiguda Station.
            </p>
          </div>
        </div>

        <!-- Timeline Item 4 -->
        <div class="timeline-item">
          <div class="timeline-marker"></div>
          <div class="timeline-content glass">
            <div class="timeline-date">Q3 2025</div>
            <h3 class="timeline-title">TransiGo 4000 Launch</h3>
            <p class="timeline-description">
              India's first instant, full-KYC, personalized bank card issuance—outside a bank branch. Complete biometric verification and card personalization in under 45 seconds.
            </p>
          </div>
        </div>
      </div>
    </div>
  </section>

  <div class="section-divider"></div>

  <!-- Success Metrics -->
  <section class="section bg-glass-light">
    <div class="container">
      <h2 class="text-center mb-10 md:mb-16">
        By the <span class="text-green">Numbers</span>
      </h2>
      <div class="grid grid-cols-1 md:grid-cols-3 gap-3 md:gap-8">
        <div
          class="glass p-6 md:p-8 text-center hover-lift rounded-2xl shadow-lg bg-gradient-to-tr from-green/10 to-black/10"
        >
          <h3
            class="text-green text-3xl md:text-4xl font-black mb-1 md:mb-4 tracking-tight"
          >
            100K+
          </h3>
          <p class="text-xs md:text-base text-white/80">
            Daily Transactions<br />Via WhatsApp ticketing platform
          </p>
        </div>
        <div
          class="glass p-6 md:p-8 text-center hover-lift rounded-2xl shadow-lg bg-gradient-to-tr from-green/10 to-black/10"
        >
          <h3
            class="text-green text-3xl md:text-4xl font-black mb-1 md:mb-4 tracking-tight"
          >
            3 min
          </h3>
          <p class="text-xs md:text-base text-white/80">
            Card Issuance Time<br />Full-KYC, personalized bank card
          </p>
        </div>
        <div
          class="glass p-6 md:p-8 text-center hover-lift rounded-2xl shadow-lg bg-gradient-to-tr from-green/10 to-black/10"
        >
          <h3
            class="text-green text-3xl md:text-4xl font-black mb-1 md:mb-4 tracking-tight"
          >
            99.9%
          </h3>
          <p class="text-xs md:text-base text-white/80">
            System Uptime<br />Banking-grade reliability
          </p>
        </div>
      </div>
    </div>
  </section>

  <div class="section-divider"></div>

  <!-- [Updated: Frog8 Rebuild Nov 2025 - by Fizzy] -->
  <section id="case-study" class="section glass case-study-section">
    <div class="container">
      <div class="case-study-header">
        <div>
          <h2 class="case-study-title">Namma Metro · Transigo 4000</h2>
          <p class="case-study-intro">
            Bengaluru’s metro trusted Transigo to deliver the nation’s first kiosk experience inaugurated by Hon’ble PM Modi—combining instant ticketing with full-KYC card issuance.
          </p>
        </div>
        <div class="case-study-actions">
          <a href={tvm4000BrochureVideo} class="btn btn-primary">Watch Video</a>
        </div>
      </div>

      <div class="case-study-media glass">
        <video
          src={tvm4000BrochureVideo}
          class="case-study-video"
          controls
          preload="metadata"
          aria-label="Transigo deployment at Namma Metro"
        ></video>
      </div>

      <div class="case-study-metrics glass">
        <div class="metric-item">
          <span class="metric-value">100K+</span>
          <span class="metric-label">daily tickets processed</span>
        </div>
        <div class="metric-item">
          <span class="metric-value">99.9%</span>
          <span class="metric-label">uptime across stations</span>
        </div>
        <div class="metric-item">
          <span class="metric-value">85%</span>
          <span class="metric-label">digital adoption</span>
        </div>
      </div>

      <div class="case-study-partners">
        <span class="partners-label">Partners</span>
        <div class="partners-grid">
          <div class="partner-chip glass partner-chip--bmrcl">
            <img src={bmrclLogo.src} alt="BMRCL logo" />
          </div>
          <div class="partner-chip glass">
            <img src={aumInfotechLogo.src} alt="AUM Infotech logo" />
          </div>
          <div class="partner-chip glass">
            <img src={advantechLogo.src} alt="Advantech logo" />
          </div>
          <div class="partner-chip glass">
            <img src={dataaegisLogo.src} alt="Dataaegis logo" />
          </div>
          <div class="partner-chip glass" style="min-width: 140px;">
            <img src={airtelPaymentsBankLogo.src} alt="Airtel Payments Bank logo" style="max-width: 140px; width: 100%;" />
          </div>
        </div>
      </div>
    </div>
  </section>

  <div class="section-divider"></div>

  <!-- Deployment Gallery -->
  <section class="section bg-glass-light">
    <div class="container">
      <h2 class="text-center mb-10 md:mb-16">
        Deployment <span class="text-green">Gallery</span>
      </h2>
      <p class="text-center mb-8 md:mb-12 text-lg text-white/80 max-w-2xl mx-auto">
        See our kiosks in action across Namma Metro stations
      </p>
      <div class="deployment-gallery">
        <div class="gallery-item glass">
          <div class="gallery-image-placeholder">
            <svg width="100" height="100" viewBox="0 0 100 100" fill="none" xmlns="http://www.w3.org/2000/svg">
              <rect width="100" height="100" fill="rgba(167, 210, 33, 0.1)"/>
              <path d="M30 30H70V70H30V30Z" stroke="#a7d221" stroke-width="2" fill="none"/>
              <circle cx="50" cy="50" r="8" fill="#a7d221"/>
            </svg>
          </div>
          <h3 class="gallery-station">TransiGo 2000 at Cubbon Park</h3>
          <p class="gallery-location">Cubbon Park</p>
        </div>
        <div class="gallery-item glass">
          <div class="gallery-image-placeholder">
            <svg width="100" height="100" viewBox="0 0 100 100" fill="none" xmlns="http://www.w3.org/2000/svg">
              <rect width="100" height="100" fill="rgba(167, 210, 33, 0.1)"/>
              <path d="M30 30H70V70H30V30Z" stroke="#a7d221" stroke-width="2" fill="none"/>
              <circle cx="50" cy="50" r="8" fill="#a7d221"/>
            </svg>
          </div>
          <h3 class="gallery-station">MG Road Station Deployment</h3>
          <p class="gallery-location">MG Road</p>
        </div>
        <div class="gallery-item glass">
          <div class="gallery-image-placeholder">
            <svg width="100" height="100" viewBox="0 0 100 100" fill="none" xmlns="http://www.w3.org/2000/svg">
              <rect width="100" height="100" fill="rgba(167, 210, 33, 0.1)"/>
              <path d="M30 30H70V70H30V30Z" stroke="#a7d221" stroke-width="2" fill="none"/>
              <circle cx="50" cy="50" r="8" fill="#a7d221"/>
            </svg>
          </div>
          <h3 class="gallery-station">Byappanahalli Customer Service</h3>
          <p class="gallery-location">Byappanahalli</p>
        </div>
        <div class="gallery-item glass">
          <div class="gallery-image-placeholder">
            <svg width="100" height="100" viewBox="0 0 100 100" fill="none" xmlns="http://www.w3.org/2000/svg">
              <rect width="100" height="100" fill="rgba(167, 210, 33, 0.1)"/>
              <path d="M30 30H70V70H30V30Z" stroke="#a7d221" stroke-width="2" fill="none"/>
              <circle cx="50" cy="50" r="8" fill="#a7d221"/>
            </svg>
          </div>
          <h3 class="gallery-station">SV Road Station Installation</h3>
          <p class="gallery-location">SV Road</p>
        </div>
        <div class="gallery-item glass">
          <div class="gallery-image-placeholder">
            <svg width="100" height="100" viewBox="0 0 100 100" fill="none" xmlns="http://www.w3.org/2000/svg">
              <rect width="100" height="100" fill="rgba(167, 210, 33, 0.1)"/>
              <path d="M30 30H70V70H30V30Z" stroke="#a7d221" stroke-width="2" fill="none"/>
              <circle cx="50" cy="50" r="8" fill="#a7d221"/>
            </svg>
          </div>
          <h3 class="gallery-station">Benniganahalli Station Overview</h3>
          <p class="gallery-location">Benniganahalli</p>
        </div>
        <div class="gallery-item glass">
          <div class="gallery-image-placeholder">
            <svg width="100" height="100" viewBox="0 0 100 100" fill="none" xmlns="http://www.w3.org/2000/svg">
              <rect width="100" height="100" fill="rgba(167, 210, 33, 0.1)"/>
              <path d="M30 30H70V70H30V30Z" stroke="#a7d221" stroke-width="2" fill="none"/>
              <circle cx="50" cy="50" r="8" fill="#a7d221"/>
            </svg>
          </div>
          <h3 class="gallery-station">Raggiguda Station</h3>
          <p class="gallery-location">Raggiguda Station</p>
        </div>
      </div>
    </div>
  </section>

  <div class="section-divider"></div>

  <!-- Testimonial Section -->
  <section class="section bg-black/20">
    <div class="container">
      <div class="testimonial-card glass">
        <div class="testimonial-quote">
          <svg class="quote-icon" width="48" height="48" viewBox="0 0 24 24" fill="none" xmlns="http://www.w3.org/2000/svg">
            <path d="M14.017 21v-7.391c0-5.704 3.731-9.57 8.983-10.609l.996 2.151c-2.432.917-3.995 3.638-3.995 5.849h4v10h-9.984zm-14.017 0v-7.391c0-5.704 3.748-9.57 9-10.609l.996 2.151c-2.433.917-3.996 3.638-3.996 5.849h3.983v10h-9.983z" fill="#a7d221" opacity="0.3"/>
          </svg>
          <p class="testimonial-text">
            "Now, get Metro tickets from new machines within 1 min"
          </p>
          <p class="testimonial-attribution">
            One passenger praised the "seamless experience" and "no queues" at our pilot deployment at Byappanahalli station.
          </p>
          <p class="testimonial-source">
            — Times of India
          </p>
        </div>
      </div>
    </div>
  </section>

  <div class="section-divider"></div>

  <!-- Partnership Section -->
  <section class="section bg-glass-light">
    <div class="container">
